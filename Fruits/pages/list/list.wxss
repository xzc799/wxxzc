page{background-color:#edf0f3;}
.nav{width:100%;height:50rpx;display:flex;flex-direction:row; background-color:#F8F8F8; }
.default{line-height:50rpx;text-align:center;flex:1;border-right:1px solid gainsboro;color:#000;font-weight:bold;font-size:28rpx;}
.red{line-height:50rpx;text-align:center;color:#fc5558;flex:1;border-right:1px solid gainsboro;font-weight:bold;font-size:28rpx;}
.show{display:block;text-align:center;line-height:200rpx;}
.hidden{display:none;text-align:center;line-height:200px;}

.navs {
  display: flex;
  background-color:ghostwhite;
  flex-flow:row wrap;
  margin-top: 10rpx;
}

.nav-item {
  width:40%;
  display: flex;
  align-items: center;
  flex-direction: column;
  background-color:#FFFFFF; 
  padding: 15rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.nav-item .nav-image {
  width: 300rpx;
  height: 300rpx;
  

border-radius: 50%;
}

.nav-item text {
  padding-top: 5rpx;
  font-size: 30rpx;
}
